{
    "branches": [
      { "name": "package", "prerelease": false },
      { "name": "develop", "prerelease": true },
      { "name": "release/*", "prerelease": false },
      { "name": "feature/*", "prerelease": true }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "FEATURE",
              "FIX"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "subject",
              "scope"
            ]
          }
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md",
          "changelogTitle": "# Changelog",
          "changelogSections": [
            { "name": "Features", "type": "feature", "scope": "all" },
            { "name": "Features", "type": "feat", "scope": "all" },
            { "name": "Bug Fixes", "type": "fix", "section": "Bug Fixes" },
            { "name": "Breaking Changes", "type": "feat", "section": "Breaking Changes" }
          ]
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
}

