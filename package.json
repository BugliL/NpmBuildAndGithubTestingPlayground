{
  "name": "@buglil/npm-dummy-package",
  "version": "1.0.0",
  "description": "Dummy package to test github",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BugliL/NpmBuildAndGithubTestingPlayground.git"
  },
  "keywords": [
    "dummy",
    "test",
    "github",
    "actions"
  ],
  "author": "Lorenzo Bugli",
  "license": "MIT",
  "dependencies": {
    "npm": "^9.6.6",
    "webpack": "^5.82.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "webpack-cli": "^5.0.2"
  },
  "release": {
    "branches": [
      {
        "name": "package",
        "prerelease": false
      },
      {
        "name": "develop",
        "prerelease": true
      },
      {
        "name": "release/*",
        "prerelease": false
      },
      {
        "name": "feature/*",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "FEATURE",
              "FIX"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "subject",
              "scope"
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
